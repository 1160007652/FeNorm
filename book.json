{
  "title": "Fe-Norm",
  "description": "前端规范",
  "author": "Muniz",
  "language": "zh-hans",
  "root": ".",

  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "anchor-navigation-ex-toc",
    "expandable-chapters",
    "chapter-fold",
    "splitter",
